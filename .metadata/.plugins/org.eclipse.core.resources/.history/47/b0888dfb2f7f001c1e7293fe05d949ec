#ifndef MCAL_I2C_I2C_PRV_H_
#define MCAL_I2C_I2C_PRV_H_

#define I2C_CLR_INTERRUPT_FLAG		8
#define I2C_SEND_STOP_CONDITION		9
#define I2C_SEND_START_CONDITION	10


#define I2C_ADDRESSES_LIMIT			120

#define I2C_PRESCALAR_MASK			0b11111000
#endif /* MCAL_I2C_I2C_PRV_H_ */
