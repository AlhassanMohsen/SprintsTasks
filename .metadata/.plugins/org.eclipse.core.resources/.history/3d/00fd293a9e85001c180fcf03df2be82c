#ifndef APP_CARD_PRV_H_
#define APP_CARD_PRV_H_

#define CARD_EEPROM_FIRST_TIME_ADDRESS 	0xFF
#define CARD_EEPROM_FIRST_TIME_DATA	   	0xA1
#define CARD_EEPROM_ADDRESS				0x00
#define CARD_HOLDER_NAME_ADDRESS		0x01
#define CARD_PAN_ADDRESS				0x11
#define CARD_PIN_ADDRESS				0x41
#define CARD_EEPROM_ADDRESS				0x00




#endif /* APP_CARD_PRV_H_ */
