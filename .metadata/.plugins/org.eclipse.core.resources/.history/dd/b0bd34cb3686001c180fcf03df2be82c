void EXTI_vidInit()
{

}
uint8_t EXTI_u8Enable(EXTINumber_t u8InterruptNumber, uint8_t u8InterruptTrigger)
{
	uint8_t u8ErrorState = EXTI_OK;

	switch(u8InterruptNumber)
	{
	case INT0:
		break;
	case INT1:
		break;
	case INT2:
		break;
	default:
		u8ErrorState =EXTI_WRONG_INT_NUMBER;

	}

	return u8ErrorState;
}

