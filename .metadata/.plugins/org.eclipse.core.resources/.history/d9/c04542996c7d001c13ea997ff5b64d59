#include "MCAL/UART/UART_interface.h"


int main (void)
{

	UART_u8Init(9600);
	UART_u8EnableRecievingInterrupt(fn);
	while (1)
	{

	}
}

void fn(void)
{
	uint8_t RecievedData;
	UART_u8RecieveByte(&RecievedData);
	UART_u8SendByte(RecievedData);
}
