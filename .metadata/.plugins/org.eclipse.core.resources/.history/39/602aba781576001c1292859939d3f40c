#include "../LIB/Typedef.h"
#include "../LIB/BIT_Math.h"
#include "../ECUAL/LED/LED.h"
#include "../MCAL/DELAY/DELAY_interface.h"



int main()
{
	LED_t st_TestLed = {PORTB,PIN4};
	LED_u8Init(st_TestLed);

	while (1)
	{
		LED_u8On(st_TestLed);
		DELAY_ms(10000);
		LED_u8Off(st_TestLed);
		DELAY_ms(10000);
	}

	return 0 ;
}
