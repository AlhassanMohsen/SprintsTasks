#include "EEPROM_prv.h"
#include "EEPROM_cfg.h"
#include "EEPROM_interface.h"



uint8_t EEPROM_u8WriteByteTo(uint8_t u8ChipAddress,uint8_t u8ByteAddress,uint8_t u8Data)
{
	uint8_t u8OperationAddress;
	u8OperationAddress = (EEPROM_CONTROL_BYTE_HIGH_NIBBLE_MASK|(u8ChipAddress<<EEPROM_FIRST_CHIP_SELECT_BIT)|0x00);
	I2C_u8MasterSendByte()
}
