#ifndef ECUAL_KEYPAD_KEYPAD_CFG_H_
#define ECUAL_KEYPAD_KEYPAD_CFG_H_

#define KEYPAD_ROWS 4
#define KEYPAD_COL  4

#define COL_S   0
#define COL_MAX 4

#define ROW_S   4
#define ROW_MAX 8

#define COL_PORT  PORTB
#define ROW_PORT  PORTB

#define COL_PIN0  PIN0
#define COL_PIN1  PIN1
#define COL_PIN2  PIN2
#define COL_PIN3  PIN3

#define ROW_PIN0  PIN4
#define ROW_PIN1  PIN5
#define ROW_PIN2  PIN6
#define ROW_PIN3  PIN7


#define KEYPAD_VALUES {{7,8,9,0},{4,5,6,0},{1,2,3,0},{0,0,200,0}}

#endif /* ECUAL_KEYPAD_KEYPAD_CFG_H_ */
