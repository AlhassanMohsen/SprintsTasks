#include "../MCAL/I2C/I2C_interface.h"
#include "../MCAL/UART/UART_interface.h"
#include "../ECUAL/LED/LED.h"
#include "../ECUAL/EEPROM/EEPROM_interface.h"

int main(void)
{
	LED_t LEDT={PORTA,PIN7};
	LED_u8Init(&LEDT);
	/*///////////////////////////////////////*
	 //////////// Master Application //////*
	 *////////////////////////////////////*
	uint8_t u8ReceivedByte;
	EEPROM_Init();
	EEPROM_u8WriteByteTo(0,0x0001,'A');
	EEPROM_u8RandonReadFrom(0,0x0001,&u8ReceivedByte)
	UART_u8Init(9600);
	while (1)
	{

		LED_u8On(&LEDT);
	}
}


